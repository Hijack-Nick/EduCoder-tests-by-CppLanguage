#include<iostream>
using namespace std;
int main()
{
	int x,n;
	int num1,num11,num2,num3,num4,num5,num6,tat;
	cin >> x >> n;
	if(x>=1 && x<=5)
	{
		num1=6-x;
	}else if(x==6 || x==7)
	{
		num1=0;
	}
	num11=8-x;
	num2=n-num11;//first
	num3=num2%7;//third
	num4=num2/7;
	num5=num4*5;//second
	if(num3>=1 && num3<=5)
	{
		num6=num3;
	}else if(num3==6 )
	{
		num6=5;
	}
	tat=(num5+num6+num1)*250;
	cout << tat;
	return 0;
}
